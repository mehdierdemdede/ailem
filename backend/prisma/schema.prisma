// After updating this schema, run:
// npx prisma generate
// npx prisma migrate dev --name init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model HealthCheckLog {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  message   String
}
